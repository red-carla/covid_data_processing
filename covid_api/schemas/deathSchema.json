{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "country_name",
    "cases",
    "deaths"
  ],
  "properties": {
    "id": {
      "title": "Id",
      "type": "integer",
      "examples": [
        500
      ],
      "default": 0
    },
    "country_name": {
      "title": "Country_name",
      "type": "string",
      "default": "",
      "examples": [
        "New country"
      ],
      "pattern": "^.*$"
    },
    "cases": {
      "title": "Cases",
      "type": "integer",
      "examples": [
        211630
      ],
      "default": 0
    },
    "deaths": {
      "title": "Deaths",
      "type": "integer",
      "examples": [
        9000
      ],
      "default": 0
    }
  }
}
